Global ACSGOMH_LoadingScreen

Global ACSGOMH_LoadingScreen_ProgressBar


Procedure OpenACSGOMH_LoadingScreen(x = 0, y = 0, width = 450, height = 50)
  ACSGOMH_LoadingScreen = OpenWindow(#PB_Any, x, y, width, height, "ACSGOMH - SigScanning", #PB_Window_SystemMenu)
  ACSGOMH_LoadingScreen_ProgressBar = ProgressBarGadget(#PB_Any, 10, 10, 430, 30, 0, 15)
EndProcedure

Procedure ACSGOMH_LoadingScreen_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

